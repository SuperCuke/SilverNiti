@inherits Umbraco.Web.Mvc.UmbracoViewPage<dynamic>
@using Archetype.Models;
@{
    var model = (ArchetypeFieldsetModel)ViewData["fieldset"];
    var lng = model.GetValue<string>("longtitude");
    var lat = model.GetValue<string>("latitude");
    var title = model.GetValue<string>("title");
    var content = model.GetValue<string>("content");

    <div class="map-outer">
        <!--Map Canvas-->
        <div class="map-canvas"
             data-zoom="12"
             data-lat="55.77"
             data-lng="37.57"
             data-type="roadmap"
             data-hue="#ffc400"
             data-title="@title"
             data-content="@content"
             style="height:420px;">
        </div>
    </div>
    <script src="http://maps.google.com/maps/api/js?key=AIzaSyChXpvpYA5yt7x8CjLkqV4FK2KWXUa2fpw"></script>
    <script src="/js/map-script.js"></script>
}

